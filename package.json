{
  "name": "fmdb",
  "displayName": "Free Mysql DB",
  "description": "Get free mysql databases with phpmyadmin access",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fmdb.openWeb",
    "onView:fmdb-sidebar"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fmdb.openWeb",
        "title": "FMDB: Open website"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "fmdb-sidebar-view",
          "title": "FMDB",
          "icon": "media/db.svg"
        }
      ]
    },
    "views": {
      "fmdb-sidebar-view": [
        {
          "type": "webview",
          "id": "fmdb-sidebar",
          "name": "FMDB",
          "icon": "media/db.svg",
          "contextualTitle": "FMDB"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "path": "^0.12.7",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.38.2",
    "svelte-check": "^2.2.0",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "publisher": "Radu",
  "repository": {
    "url": "https://github.com/coderadu/fmdb"
  }
}
